$ exec git init -q .
$ exec git config --global user.email "you@example.com"
$ exec git config --global user.name "Your Name"
$ exec git checkout -b main
$ exec git add .
$ exec git commit -m init

$ exec git checkout -b mybranch
$ touch file2
$ exec git add file2
$ exec git commit -am "added file2"

> releaseNotesDraft

$ exists release-notes/release_mybranch.md
$ must-mirror release-notes/release_mybranch.md expected.md